def hide_card(card_number):

    card_number = card_number.replace(' ', '')
    card_number = 12 * '*' + card_number[12:]
    
    return card_number

'''
input:
card = '1234567890123456'

print(hide_card(card))

output:
************1234
'''

'''
input:
card = '905 678123 45612 56'

print(hide_card(card))

output:
************1256
'''
